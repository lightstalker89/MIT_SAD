<Window x:Class="Resist.ColorsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ColorsWindow" Height="300" Width="300"
        DataContext="{Binding RelativeSource={RelativeSource Self}}"
        >
    <Grid >
        <StackPanel>
            <Button Command="{Binding Selector}" CommandParameter="0">schwarz</Button>
            <Button Command="{Binding Selector}" CommandParameter="1">braun</Button>
            <Button Command="{Binding Selector}" CommandParameter="2">rot</Button>
            <Button Command="{Binding Selector}" CommandParameter="3">orange</Button>
            <Button Command="{Binding Selector}" CommandParameter="4">gelb</Button>
            <Button Command="{Binding Selector}" CommandParameter="5">grün</Button>
            <Button Command="{Binding Selector}" CommandParameter="6">blau</Button>
            <Button Command="{Binding Selector}" CommandParameter="7">violett</Button>
            <Button Command="{Binding Selector}" CommandParameter="8">grau</Button>
            <Button Command="{Binding Selector}" CommandParameter="9">weiß</Button>
            
        </StackPanel>
    </Grid>
</Window>
