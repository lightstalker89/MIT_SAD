<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<AnchorPane id="AnchorPane" prefHeight="443.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hotswapping.FXMLDocumentController">
  <children>
     <Label layoutX="14.0" layoutY="1.0" text="JavaAssist:" />
      <AnchorPane id="javaAssistPanel" layoutX="14.0" layoutY="21.0" prefHeight="225.0" prefWidth="431.0">
      <children>
        <Button id="buttonHandleJavassist" layoutX="14.0" layoutY="93.0" onAction="#handleJavassistButton" text="Insert Code" />
        <TextArea fx:id="javassistTextArea" layoutX="122.0" layoutY="19.0" prefHeight="179.0" prefWidth="293.0" promptText="Insert your code" wrapText="true" />
        <Label fx:id="javassistError" alignment="CENTER" layoutX="122.0" layoutY="200.0" text="Please insert a valid code" textFill="RED" visible="false" />
        <Label fx:id="javassistSwappingError" alignment="CENTER" layoutX="122.0" layoutY="200.0" text="Could not initialize Hot swapping" textFill="RED" visible="false" />
      </children>
    </AnchorPane>
    <Button layoutX="465.0" layoutY="114.0" mnemonicParsing="false" onAction="#executeCodeJavaAssist" text="Execute" />
    <Label layoutX="14.0" layoutY="272.0" text="Byte Buddy:" />
    <AnchorPane id="byteBuddyPanel" layoutX="14.0" layoutY="292.0" prefHeight="137.0" prefWidth="431.0">
        <children>
           <Button id="buttonHandleByteBuddy" layoutX="14.0" layoutY="53.0" onAction="#handleByteBuddyButton" text="Swap Code" />
           <TextField fx:id="field2" layoutX="247.0" layoutY="53.0" prefHeight="31.0" prefWidth="44.0" />
           <TextField fx:id="field1" layoutX="125.0" layoutY="53.0" prefHeight="31.0" prefWidth="44.0" />
            <Label fx:id="calcSign" layoutX="205.0" layoutY="58.0" text="+" />
            <Label fx:id="result" layoutX="373.0" layoutY="57.0" textFill="#68bf2e" />
            <Label layoutX="304.0" layoutY="58.0" text="=" />
          <Label fx:id="usedClassInfo" alignment="CENTER" layoutX="2.0" layoutY="102.0" prefHeight="21.0" prefWidth="431.0" text="Use AddCalculator class" />
        </children>
    </AnchorPane>
    <Button layoutX="465.0" layoutY="345.0" mnemonicParsing="false" onAction="#executeCodeByteBuddy" text="Execute" />
  </children>
  <stylesheets>
    <URL value="@cssStyles/MainWindowStyle.css" />
  </stylesheets>
</AnchorPane>
